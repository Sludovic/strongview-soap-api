{
    "name": "activenetwork/strongview-soap-api",
    "type": "library",
    "description": "PHP classes based on Selligent's Message Studio SOAP WSDL",
    "keywords": ["Selligent", "StrongView", "Message Studio", "SOAP", "API"],
    "license": "proprietary",
    "authors": [
        {
            "name": "Rhyan Randolph",
            "email": "rhyan.randolph@activenetwork.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "vlucas/phpdotenv": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "wsdl2phpgenerator/wsdl2phpgenerator": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Selligent\\": "src/Selligent"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
